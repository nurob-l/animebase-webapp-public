{
  "name": "animebase-webapp",
  "sideEffects": [
    "./src/some-side-effectful-file.js",
    "./src/app.js",
    "./src/entry-client.js",
    "./src/entry-server.js",
    "*.css",
    "*.s[ac]ss",
    "*.vue"
  ],
  "version": "1.0.0",
  "description": "提供web客户端，将数据服务器与app服务器分开",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:start": "cross-env NODE_ENV=production node server",
    "dev": "node server",
    "start": "cross-env NODE_ENV=production pm2 start pm2.conf.json",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production VUE_ENV=client webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production VUE_ENV=server webpack --config build/webpack.server.config.js --progress --hide-modules",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nurob-l/animebase-webapp.git"
  },
  "author": "nurob",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nurob-l/animebase-webapp/issues"
  },
  "homepage": "https://github.com/nurob-l/animebase-webapp#readme",
  "dependencies": {
    "@babel/runtime": "^7.10.2",
    "ali-oss": "^6.9.1",
    "axios": "^0.19.2",
    "echarts": "^4.8.0",
    "element-ui": "^2.13.2",
    "swiper": "^5.4.5",
    "uuid": "^8.1.0",
    "vue": "^2.6.11",
    "vue-awesome-swiper": "^4.1.1",
    "vue-cropper": "^0.5.4",
    "vue-meta": "^2.4.0",
    "vue-router": "^3.3.2",
    "vue-server-renderer": "^2.6.11",
    "vuetify": "^2.3.9",
    "vuex": "^3.4.0",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "autoprefixer": "^9.8.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-component": "^1.1.1",
    "chokidar": "^3.4.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "deepmerge": "^4.2.2",
    "fibers": "^5.0.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "koa": "^2.12.0",
    "koa-bodyparser": "^4.3.0",
    "koa-compress": "^4.0.1",
    "koa-favicon": "^2.1.0",
    "koa-router": "^9.0.1",
    "koa-webpack-middleware": "^1.0.7",
    "lru-cache": "^5.1.1",
    "memory-fs": "^0.5.0",
    "mime": "^2.4.6",
    "mini-css-extract-plugin": "^0.9.0",
    "mz": "^2.7.0",
    "postcss-loader": "^3.0.0",
    "rimraf": "^3.0.2",
    "sass": "^1.26.10",
    "sass-loader": "^9.0.2",
    "sw-precache-webpack-plugin": "^1.0.0",
    "vue-loader": "^15.9.2",
    "vue-template-compiler": "^2.6.11",
